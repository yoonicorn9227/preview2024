                <input type="email" required placeholder=" ☞ E-mail을 입력하세요.">
              </div>
              <div class="msg">
                <div class="text">Message *</div>
                <textarea rows="2" cols="25" required placeholder="※ 문의사항을 입력하세요."></textarea>
              </div>
                <button type="submit" class="btn">Send</button>
            </form>
